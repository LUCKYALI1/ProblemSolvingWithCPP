string removeOuterParentheses(string s) {
       int size = s.length();
       string ans ;
       int balance = 0;
       for(int i = 0 ; i < size ; i++){
          if( s[i] == '('){
            if(balance > 0){
                ans.push_back(s[i]);
            }
            balance++;
          }
          if(s[i] == ')'){
            balance--;
            if(balance > 0){
                ans.push_back(s[i]);
            }
          }
       }
       return ans;
    }
